{
    "version": 2,
    "builds": [
      {
        "src": "firstproject/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "../static_in_env"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static_in_pro/$1"
      },
      {
        "src": "/(.*)",
        "dest": "firstproject/wsgi.py"
      }
    ]
  }

